FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    LeftWedge
    {
        type            wedge;
    }
    RightWedge
    {
        type            wedge;
    }
    Inlet
    {
        type            zeroGradient;
    }
    Outlet
    {
        type            zeroGradient;
    }
    Outterwall
    {
        type            zeroGradient;
    }
    fluid_to_solid
    {
        type            calculated;
        value           uniform 0;
    }
}


// ************************************************************************* //
