FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/cilcen";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 2;

boundaryField
{
    ciltopcen
    {
        type            zeroGradient;
    }
    cilbotcen
    {
        type            zeroGradient;
    }
    symmetrycen1
    {
        type            symmetry;
    }
    symmetrycen2
    {
        type            symmetry;
    }
    cilcen_to_fluid
    {
        type            calculated;
        value           uniform 0;
    }
}


