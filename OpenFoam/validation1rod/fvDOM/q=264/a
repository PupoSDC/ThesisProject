FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "2498";
    object      a;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -1 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    LeftWedge
    {
        type            wedge;
    }
    RightWedge
    {
        type            wedge;
    }
    Inlet
    {
        type            calculated;
        value           uniform 0;
    }
    Outlet
    {
        type            calculated;
        value           uniform 0;
    }
    Outterwall
    {
        type            calculated;
        value           uniform 0;
    }
    Innerwall
    {
        type            calculated;
        value           uniform 0;
    }
}


