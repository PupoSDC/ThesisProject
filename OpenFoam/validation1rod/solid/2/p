FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "2";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    cencil_to_fluid
    {
        type            fixedValue;
        value           uniform 101325;
    }
    cilcentop
    {
        type            fixedValue;
        value           uniform 101325;
    }
    cilcenbot
    {
        type            fixedValue;
        value           uniform 101325;
    }
    symmetryleftcen
    {
        type            symmetry;
    }
    symmetryrigtcen
    {
        type            symmetry;
    }
    defaultFaces
    {
        type            empty;
    }
}


